{
  "name": "@forthic/interp",
  "version": "0.6.0",
  "description": "Forthic interpreter",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "pretty": "prettier --print-width 120 --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json}\"",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build": "npm run build:cjs && npm run build:esm",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/linkedin/forthic.git"
  },
  "author": "Rino Jose",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/linkedin/forthic/issues"
  },
  "homepage": "https://github.com/linkedin/forthic#readme",
  "devDependencies": {
    "@babel/cli": "^7.25.9",
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@eslint/js": "^9.15.0",
    "@types/jest": "^29.5.14",
    "eslint": "^9.15.0",
    "globals": "^15.12.0",
    "jest": "^29.7.0",
    "prettier": "3.4.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.16.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
